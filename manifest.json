{
  "update_url": "https://files.tradition1871.com/oneTwoOne/update.xml",
  "manifest_version": 3,
  "name": "oneTwoOne Lockdown",
  "short_name": "oneTwoOne",
  "description": "Prevents students from logging into other student Chromebooks.",
  "version": "2.0",
  "background": {
    "service_worker": "background.js"
  },
  "minimum_chrome_version": "66",
  "permissions": [
    "identity",
    "identity.email",
    "enterprise.deviceAttributes",
    "<all_urls>",
    "webRequest",
    "declarativeNetRequestWithHostAccess"
  ]
}